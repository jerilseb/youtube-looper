{
    "manifest_version": 2,
    "name": "Youtube Looper",
    "description": "Loop YouTube videos between two selected points",
    "version": "1.0",
    "minimum_chrome_version": "66.0",
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png",
      "256": "icons/256.png"
    },
    "content_scripts": [
      {
        "matches": ["https://youtube.com/*", "https://www.youtube.com/*"],
        "js": ["js/main.js"],
        "css": ["main.css"],
        "run_at": "document_end"
      }
    ]
}